<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Que1Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <GridPane fx:id="grid" alignment="TOP_CENTER" centerShape="true" scaleShape="true">
      </GridPane>
      <Label fx:id="lbl"/>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="yes" alignment="TOP_RIGHT" mnemonicParsing="false" onAction="#PlayNewGame" text="Yes" visible="false" />
            <Button fx:id="no" mnemonicParsing="false" onAction="#CloseApp" text="No" visible="false" />
         </children>
      </HBox>
      <ImageView fx:id="img" fitHeight="-Infinity" fitWidth="-Infinity"/>
       <Label fx:id="his" />
   </children>
</VBox>
